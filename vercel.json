{
    "redirects": [
        {
            "source": "/((?!about|contact|pricing|how-it-works|blog|features|assets|_astro|favicon).*)",
            "destination": "https://app.figmafolio.com/$1",
            "permanent": false
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}